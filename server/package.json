{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "graphql server for the projects time tracking project",
  "scripts": {
    "start": "nodemon"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "nanoid": "^3.1.22"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "ts-node -r ./src",
    "ext": "ts"
  },
  "type": "module",
  "author": "Raphael Terrier @R4ph-t",
  "license": "MIT",
  "private": true
}
